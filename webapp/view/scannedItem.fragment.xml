<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog title="Scanned Barcode Details">
		<l:Grid defaultSpan="L12 M12 S12" width="auto" id="idGrid_2">
			<l:content>
				<f:SimpleForm id="SimpleFormDisplay354_2" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="3"
